{
    "name": "darwinluague/translator",
    "description": "Laravel's localization management package.",
    "keywords": [
        "laravel",
        "translation",
        "localization"
    ],
    "type": "management",
    "license": "MIT",
    "authors": [
        {
            "name": "Darwin Luague",
            "email": "darwinluague9001@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "DarwinLuague\\Translator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarwinLuague\\Translator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "DarwinLuague\\Translator\\TranslatorServiceProvider"
            ],
            "aliases": {
                "Translator": "DarwinLuague\\Translator\\Translator"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
